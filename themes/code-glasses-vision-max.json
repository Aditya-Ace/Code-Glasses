    "terminal.foreground": "#EADBCF",
    "terminalCursor.background": "#0D0D0D",
    "terminalCursor.foreground": "#FBC154",
    "terminal.selectionBackground": "#8A8AA133",
    "terminal.ansiBlack": "#1A1A1A",
    "terminal.ansiBrightBlack": "#6B6B7F",
    "terminal.ansiRed": "#C75D4E",
    "terminal.ansiBrightRed": "#FF7A6C",
    "terminal.ansiGreen": "#A7C484",
    "terminal.ansiBrightGreen": "#C5E1A5",
    "terminal.ansiYellow": "#FBC154",
    "terminal.ansiBrightYellow": "#FFF176",
    "terminal.ansiBlue": "#8A8AA1",
    "terminal.ansiBrightBlue": "#B39DDB",
    "terminal.ansiMagenta": "#E88E5A",
    "terminal.ansiBrightMagenta": "#F48FB1",
    "terminal.ansiCyan": "#79B8FF",
    "terminal.ansiBrightCyan": "#80CBC4",
    "terminal.ansiWhite": "#EADBCF",
    "terminal.ansiBrightWhite": "#FFFFFF"
  },
  "tokenColors": [
    {
      "name": "Default",
      "scope": ["source", "text"],
      "settings": { "foreground": "#EADBCF" }
    },
    {
      "name": "Invalid / Illegal",
      "scope": ["invalid", "invalid.illegal"],
      "settings": { "foreground": "#FF7A6C" }
    },
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": { "fontStyle": "italic", "foreground": "#6B6B7F" }
    },
    {
      "name": "Documentation Markup",
      "scope": ["comment.block.documentation"],
      "settings": { "foreground": "#8A8AA1" }
    },
    {
      "name": "Strings",
      "scope": ["string", "punctuation.definition.string"],
      "settings": { "foreground": "#C75D4E" }
    },
    {
      "name": "String Templates",
      "scope": ["string.template", "punctuation.definition.template-string"],
      "settings": { "foreground": "#C75D4E" }
    },
    {
      "name": "Regex",
      "scope": ["string.regexp", "constant.character.escape"],
      "settings": { "foreground": "#79B8FF" }
    },
    {
      "name": "Numbers & Booleans",
      "scope": ["constant.numeric", "constant.language"],
      "settings": { "foreground": "#E88E5A" }
    },
    {
      "name": "Other Constants",
      "scope": ["constant.character", "constant.other"],
      "settings": { "foreground": "#E88E5A" }
    },
    {
      "name": "Keywords (Control Flow)",
      "scope": ["keyword.control", "keyword.operator.new"],
      "settings": { "fontStyle": "italic", "foreground": "#FBC154" }
    },
    {
      "name": "Logical/Comparison Operators",
      "scope": ["keyword.operator.logical", "keyword.operator.comparison"],
      "settings": { "foreground": "#8A8AA1" }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": { "foreground": "#6B6B7F" }
    },
    {
      "name": "Punctuation",
      "scope": ["punctuation", "meta.brace", "meta.delimiter"],
      "settings": { "foreground": "#6B6B7F" }
    },
    {
      "name": "Variables",
      "scope": ["variable", "meta.definition.variable"],
      "settings": { "foreground": "#EADBCF" }
    },
    {
      "name": "Read/Write Variables",
      "scope": ["variable.other.readwrite"],
      "settings": { "foreground": "#EADBCF" }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": { "fontStyle": "italic", "foreground": "#EADBCF" }
    },
    {
      "name": "Object Properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": { "foreground": "#EADBCF" }
    },
    {
      "name": "Property Names (CSS/JSON/YAML)",
      "scope": [
        "support.type.property-name",
        "meta.object-literal.key",
        "support.type.property-name.json"
      ],
      "settings": { "foreground": "#8A8AA1" }
    },
    {
      "name": "Functions & Methods (Decl)",
      "scope": ["entity.name.function", "support.function"],
      "settings": { "foreground": "#A7C484" }
    },
    {
      "name": "Function & Method Calls",
      "scope": [
        "meta.function-call",
        "entity.name.function.call",
        "support.function.console"
      ],
      "settings": { "foreground": "#A7C484" }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": { "fontStyle": "bold", "foreground": "#8A8AA1" }
    },
    {
      "name": "Inherited Classes",
      "scope": ["entity.other.inherited-class"],
      "settings": { "fontStyle": "italic bold", "foreground": "#8A8AA1" }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type",
        "support.type",
        "entity.name.interface",
        "storage.type"
      ],
      "settings": { "foreground": "#8A8AA1" }
    },
    {
      "name": "Namespaces & Modules",
      "scope": [
        "entity.name.namespace",
        "entity.name.module",
        "entity.name.package"
      ],
      "settings": { "foreground": "#8A8AA1" }
    },
    {
      "name": "Component Tags (React/Angular/Vue)",
      "scope": ["support.class.component", "support.type.component"],
      "settings": { "fontStyle": "bold", "foreground": "#8A8AA1" }
    },
    {
      "name": "HTML/JSX/TSX Tags",
      "scope": ["entity.name.tag"],
      "settings": { "foreground": "#C75D4E" }
    },
    {
      "name": "Tag Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": { "fontStyle": "italic", "foreground": "#E88E5A" }
    },
    {
      "name": "Decorators/Annotations",
      "scope": [
        "meta.decorator",
        "storage.type.annotation",
        "punctuation.decorator"
      ],
      "settings": { "fontStyle": "italic", "foreground": "#FBC154" }
    },
    {
      "name": "Preprocessor Directives",
      "scope": ["keyword.control.directive"],
      "settings": { "fontStyle": "italic", "foreground": "#6B6B7F" }
    },
    {
      "name": "Markdown Headings",
      "scope": [
        "markup.heading",
        "markup.heading.setext",
        "entity.name.section"
      ],
      "settings": { "foreground": "#FBC154" }
    },
    {
      "name": "Markdown Emphasis",
      "scope": ["markup.bold", "markup.italic"],
      "settings": { "foreground": "#EADBCF" }
    },
    {
      "name": "Markdown Code",
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": { "foreground": "#A7C484" }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#EADBCF",
    "property.readonly": "#EADBCF",
    "parameter": { "foreground": "#EADBCF", "italic": true },
    "function": "#A7C484",
    "method": "#A7C484",
    "class": { "foreground": "#8A8AA1", "bold": true },
    "struct": "#8A8AA1",
    "enum": "#8A8AA1",
    "enumMember": "#E88E5A",
    "interface": "#8A8AA1",
    "namespace": "#8A8AA1",
    "typeParameter": "#8A8AA1",
    "decorator": { "foreground": "#FBC154", "italic": true },
    "event": "#FBC154",
    "regexp": "#79B8FF",
    "operator": "#6B6B7F",
    "keyword": { "foreground": "#FBC154", "italic": true },
    "comment": { "foreground": "#6B6B7F", "italic": true },
    "string": "#C75D4E",
    "number": "#E88E5A",
    "boolean": "#E88E5A",
    "type": "#8A8AA1"
  }
}
